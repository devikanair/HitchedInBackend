<h1>Listing changetypes</h1>

<table>
  <tr>
    <th>Random</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @changetypes.each do |changetype| %>
  <tr>
    <td><%= changetype.random %></td>
    <td><%= link_to 'Show', changetype %></td>
    <td><%= link_to 'Edit', edit_changetype_path(changetype) %></td>
    <td><%= link_to 'Destroy', changetype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Changetype', new_changetype_path %>
